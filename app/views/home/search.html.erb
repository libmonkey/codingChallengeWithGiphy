<div class="album py-5">
    <div class="container">

      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4 g-4">
          <%= render partial: 'giphys/giphy_card', collection: @giphys, as: :giphy %>
      </div>
    </div>

    <div class="container">
      <nav aria-label="Page navigation example" >
        <%= will_paginate @giphys, renderer: BootstrapPagination::Rails %>
      </nav>
    </div>
</div>

<%= render partial: 'giphys/giphy_modal', collection: @giphys, as: :giphy %>
